<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="processors"
    nsURI="http:///ca/gc/space/mrt/common/model/processors.ecore" nsPrefix="ca.gc.space.mrt.common">
  <eClassifiers xsi:type="ecore:EClass" name="Monitorable" abstract="true" interface="true">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Represent a object for which operation progress can be reported."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="progressMonitor" eType="#//IProgressMonitor"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EDataType" name="IProgressMonitor" instanceClassName="org.eclipse.core.runtime.IProgressMonitor"/>
  <eClassifiers xsi:type="ecore:EClass" name="Processor" abstract="true" interface="true"
      eSuperTypes="#//Monitorable">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Defines an object that can process an input to get an output."/>
    </eAnnotations>
    <eOperations name="process" lowerBound="1" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EJavaObject">
      <eParameters name="input" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EJavaObject"/>
    </eOperations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ProcessorsChain" eSuperTypes="#//Processor">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Defines a chain of Processors that are applied in sequence on an input to get an ouptut."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="intermediateResult" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EJavaObject"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="processors" unique="false"
        upperBound="-1" eType="#//Processor"/>
  </eClassifiers>
</ecore:EPackage>
