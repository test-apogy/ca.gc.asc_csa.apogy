<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
 Canadian Space Agency 2011
-->

<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://ca/gc/asc_csa/eclipse/images/core/1.0"
            class="org.eclipse.symphony.common.images.ImagesCorePackage"
            genModel="model/ca_gc_asc_csa_eclipse_images.genmodel"/>
   </extension>
   <extension
         point="org.eclipse.symphony.common.converters">
      <new_element
            Class="org.eclipse.symphony.common.images.converters.IFileToImageData">
      </new_element>
      <new_element
            Class="org.eclipse.symphony.common.images.converters.ObjectToImageDataConverter">
      </new_element>
      <new_element
            Class="org.eclipse.symphony.common.images.converters.EImageToFileConverter">
      </new_element>
      <new_element
            Class="org.eclipse.symphony.common.images.converters.ObjectToBufferedImageConverter">
      </new_element>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated ImagesCore -->
      <package
            uri="http://ca/gc/asc_csa/eclipse/images/core/1.0"
            class="ca.gc.asc_csa.eclipse.images.core.ImagesCorePackage"
            genModel="model/symphony_common_images.xcore"/>
   </extension>

</plugin>
