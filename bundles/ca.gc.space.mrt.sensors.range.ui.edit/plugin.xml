<plugin>


   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <!-- @generated mrt_range_ui -->
      <factory
            uri="ca.gc.space.mrt.sensors.range.ui"
            class="ca.gc.space.mrt.sensors.range.ui.provider.RangeSensorsUIItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>

   <extension point="org.eclipse.emf.edit.childCreationExtenders">
      <!-- @generated mrt_range_ui -->
      <extender
            uri="ca.gc.space.topology.ui"
            class="ca.gc.space.mrt.sensors.range.ui.provider.RangeSensorsUIItemProviderAdapterFactory$TopologyUIChildCreationExtender"/>
      <extender
            uri="ca.gc.space.topology"
            class="ca.gc.space.mrt.sensors.range.ui.provider.RangeSensorsUIItemProviderAdapterFactory$TopologyChildCreationExtender"/>
   </extension>

</plugin>
