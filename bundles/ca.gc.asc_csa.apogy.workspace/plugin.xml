<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated apogy_core_workspace -->
      <package
            uri="ca.gc.asc_csa.apogy.workspace"
            class="ca.gc.asc_csa.apogy.workspace.ApogyWorkspacePackage"
            genModel="model/apogy_workspace.xcore"/>
   </extension>
   
   <extension
         id="ca.gc.asc_csa.apogy.workspace.natures.ApogyNature"
         name="Apogy"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="ca.gc.asc_csa.apogy.workspace.natures.ApogyProjectNature">
         </run>
      </runtime>
   </extension>   
   <extension
         point="org.eclipse.ui.ide.projectNatureImages">
      <image
            icon="icons/ovr/apogy_7x8.gif"
            id="ca.gc.asc_csa.apogy.core.ui.projectNatureImages"
            natureId="ca.gc.asc_csa.apogy.core.ui.natures.ApogyNature">
      </image>
   </extension>
</plugin>
