<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="recorders" nsURI="http:///ca/gc/space/mrt/common/recorders/model/recorders.ecore"
    nsPrefix="ca.gc.space.mrt.common">
  <eClassifiers xsi:type="ecore:EClass" name="Recordings">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Represent a object that is used to stored data generated by a Recorder. This includes the source object (source), the names of the source object's features being recorded (tracks), and the actual time tag features values themselves (records)."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EObject"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="records" lowerBound="1"
        transient="true">
      <eGenericType eClassifier="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EMap">
        <eTypeArguments eClassifier="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDate"/>
        <eTypeArguments eClassifier="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EEList">
          <eTypeArguments eClassifier="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EJavaObject"/>
        </eTypeArguments>
      </eGenericType>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="tracks" upperBound="-1"
        eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Recorder" eSuperTypes="platform:/plugin/org.eclipse.symphony.common.processors/model/processors.ecore#//Monitorable">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Represent a object that can monitor an object and record its changes through time."/>
    </eAnnotations>
    <eOperations name="isToBeRecorded" lowerBound="1" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EBoolean">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Returns wether or not a new value for a specific track should be recorded or not. This method is intended to be overloaded by users that want to add restriction on what is considered a &quot;new value&quot;. The default implementation always returns true."/>
      </eAnnotations>
      <eParameters name="newValue" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EJavaObject"/>
      <eParameters name="track" lowerBound="1" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EString"/>
    </eOperations>
    <eOperations name="terminate" lowerBound="1"/>
    <eOperations name="resume" lowerBound="1"/>
    <eOperations name="pause" lowerBound="1"/>
    <eOperations name="start" lowerBound="1"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="status" lowerBound="1"
        eType="#//RecorderStatus"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="RecorderStatus">
    <eLiterals name="INITIALIZING" literal=""/>
    <eLiterals name="RECORDING" value="1"/>
    <eLiterals name="PAUSED" value="2"/>
    <eLiterals name="TEMINATED" value="3"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RecordingsExporter" abstract="true"
      interface="true">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Represent a object that is used to save to file the data acquired by a Recorder."/>
    </eAnnotations>
    <eOperations name="export" lowerBound="1">
      <eParameters name="path" lowerBound="1" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EString"/>
    </eOperations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="recordings" eType="#//Recordings"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CsvExporter">
    <eOperations name="exportToCSVFile" lowerBound="1">
      <eParameters name="exportPath" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eOperations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="includeColumnDescription"
        lowerBound="1" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="includeColumnName" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="columnSet" lowerBound="1"
        eType="ecore:EClass platform:/plugin/org.eclipse.symphony.common.file.csv/model/csv.ecore#//CsvColumsSet"
        changeable="false" derived="true"/>
  </eClassifiers>
</ecore:EPackage>
