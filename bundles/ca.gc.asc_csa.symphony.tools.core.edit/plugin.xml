<plugin>


   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <!-- @generated symphony_tools_core -->
      <factory
            uri="ca.gc.asc_csa.symphony.tools.core"
            class="ca.gc.asc_csa.symphony.tools.core.provider.SymphonyToolsCoreItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>

   <extension point="org.eclipse.emf.edit.childCreationExtenders">
      <!-- @generated symphony_tools_core -->
      <extender
            uri="ca.gc.asc_csa.topology.bindings"
            class="ca.gc.asc_csa.symphony.tools.core.provider.SymphonyToolsCoreItemProviderAdapterFactory$TopologyBindingsChildCreationExtender"/>
      <extender
            uri="org.eclipse.symphony.core"
            class="ca.gc.asc_csa.symphony.tools.core.provider.SymphonyToolsCoreItemProviderAdapterFactory$SymphonyCoreChildCreationExtender"/>
      <extender
            uri="http:///ca/gc/space/topology.ecore"
            class="ca.gc.asc_csa.symphony.tools.core.provider.SymphonyToolsCoreItemProviderAdapterFactory$TopologyChildCreationExtender"/>
      <extender
            uri="org.eclipse.symphony.core.invocator"
            class="ca.gc.asc_csa.symphony.tools.core.provider.SymphonyToolsCoreItemProviderAdapterFactory$EMFEcoreInvocatorChildCreationExtender"/>
      <extender
            uri="http:///ca/gc/space/mrt/geometry/paths/model/paths.ecore"
            class="ca.gc.asc_csa.symphony.tools.core.provider.SymphonyToolsCoreItemProviderAdapterFactory$PathsChildCreationExtender"/>
   </extension>

</plugin>
