    <eOperations name="getEndPoint" eType="ecore:EClass ../../org.eclipse.symphony.common.geometry.data3d/model/data3d.ecore#//CartesianPositionCoordinates"/>
    <eOperations name="getStartPoint" eType="ecore:EClass ../../org.eclipse.symphony.common.geometry.data3d/model/data3d.ecore#//CartesianPositionCoordinates"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="lenght" lowerBound="1"
