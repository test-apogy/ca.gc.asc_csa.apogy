      <eTypeArguments eClassifier="#//WayPointPath"/>
      <eTypeArguments eClassifier="#//WayPointPath"/>
    </eGenericSuperTypes>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="MinimumDistanceFilter" eSuperTypes="#//WayPointPathFilter">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="minimumDistance" eType="ecore:EDataType ../../org.eclipse.emf.ecore/model/Ecore.ecore#//EDouble"/>
  </eClassifiers>
