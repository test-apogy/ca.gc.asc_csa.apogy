  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CatmullRomWayPointPathInterpolator"
      eSuperTypes="#//WayPointPathInterpolator">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Fits a spline on the specified path using Catmull-Rom and generates an interpolated path using the specified maximum point to point distance.&#xA;The original waypoints are included in the resulting interpolated path ?"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="tension" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble" defaultValueLiteral="0.5"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="maximumWayPointsDistance"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble" defaultValueLiteral="1.0"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="endControlPointGenerationMode"
        eType="#//SplineEndControlPointGenerationMode" defaultValueLiteral="AUTO_CTRL_POINTS_DUPLICATE_ENDNODES"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="WayPointPathFilter" abstract="true"
