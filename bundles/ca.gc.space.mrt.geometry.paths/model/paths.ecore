    <eLiterals name="AUTO_CTRL_POINTS_REFLECTION" value="2"/>
    <eLiterals name="AUTO_CTRL_POINTS_CLOSE_LOOPS" value="3"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SegmentWayPointPathInterpolator" eSuperTypes="#//WayPointPathInterpolator">
