    <eOperations name="createWayPointPath" eType="#//WayPointPath">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Creates a WayPointPath from a WayPointPath. The WayPointPath created contains copies of all the points found in the specified WayPointPath."/>
      </eAnnotations>
      <eParameters name="wayPointPath" eType="#//WayPointPath"/>
    </eOperations>
    <eOperations name="createWayPointPath" eType="#//WayPointPath">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Creates a WayPointPath using a list of CartesianPositionCoordinates. The WayPointPath created contains copies of all the points found in the specified list."/>
      </eAnnotations>
      <eParameters name="points">
        <eGenericType eClassifier="#//List">
          <eTypeArguments eClassifier="ecore:EClass ../../org.eclipse.symphony.common.geometry.data3d/model/data3d.ecore#//CartesianPositionCoordinates"/>
        </eGenericType>
      </eParameters>
    </eOperations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="SplineEndControlPointGenerationMode">
