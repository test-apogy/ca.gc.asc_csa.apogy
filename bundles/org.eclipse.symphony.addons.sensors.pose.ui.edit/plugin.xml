<plugin>


   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="org.eclipse.symphony.addons.sensors.pose.ui"
            class="org.eclipse.symphony.addons.sensors.pose.ui.provider.SensorsPoseUIItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>

   <extension point="org.eclipse.emf.edit.childCreationExtenders">
      <extender
            uri="org.eclipse.symphony.common.topology.ui"
            class="org.eclipse.symphony.addons.sensors.pose.ui.provider.SensorsPoseUIItemProviderAdapterFactory$TopologyUIChildCreationExtender"/>
      <extender
            uri="org.eclipse.symphony.common.topology"
            class="org.eclipse.symphony.addons.sensors.pose.ui.provider.SensorsPoseUIItemProviderAdapterFactory$TopologyChildCreationExtender"/>
   </extension>

</plugin>
