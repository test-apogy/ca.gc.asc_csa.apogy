<plugin>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <!-- @generated mrt_fov_bindings -->
      <factory
            uri="ca.gc.space.mrt.sensors.fov.bindings"
            class="ca.gc.space.mrt.sensors.fov.bindings.provider.MRTFOVBindingsItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
