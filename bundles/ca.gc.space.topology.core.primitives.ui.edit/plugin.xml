<plugin>


   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <!-- @generated topology_primitives_ui -->
      <factory
            uri="ca.gc.space.topology.core.primitives.ui"
            class="ca.gc.space.topology.core.primitives.ui.provider.TopologyUIPrimitivesItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>

   <extension point="org.eclipse.emf.edit.childCreationExtenders">
      <!-- @generated topology_primitives_ui -->
      <extender
            uri="http:///ca/gc/space/topology.ecore"
            class="ca.gc.space.topology.core.primitives.ui.provider.TopologyUIPrimitivesItemProviderAdapterFactory$TopologyChildCreationExtender"/>
      <extender
            uri="ca.gc.space.topology.ui"
            class="ca.gc.space.topology.core.primitives.ui.provider.TopologyUIPrimitivesItemProviderAdapterFactory$TopologyUIChildCreationExtender"/>
   </extension>

</plugin>
