<plugin>
   <extension-point id="SymphonySystemContributor" name="SymphonySystemContributor" schema="schema/SymphonySystemContributor.exsd"/>
   <extension-point id="WorksiteProvider" name="WorksiteProvider" schema="schema/WorksiteProvider.exsd"/>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="ca.gc.asc_csa.symphony.core"
            class="ca.gc.asc_csa.symphony.core.SymphonyCorePackage"
            genModel="model/symphony_core.xcore"/>
   </extension>
   <extension
         point="ca.gc.asc_csa.eclipse.emf.ecore.invocator.InvocatorDelegate">
      <element
            invocatorDelegateClass="ca.gc.asc_csa.symphony.core.invocator.SymphonyInvocatorDelegate">
      </element>
   </extension>
   
   <extension point="org.eclipse.emf.ecore.extension_parser">
      <parser
            type="ss"
            class="ca.gc.asc_csa.eclipse.emf.ecore.invocator.util.EMFEcoreInvocatorResourceFactoryImpl"/>
   </extension>
</plugin>
