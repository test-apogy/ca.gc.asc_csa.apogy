<plugin>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="org.eclipse.symphony.core.recordings"
            class="org.eclipse.symphony.core.recordings.provider.EMFEcoreInvocatorRecordingsItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>

   <extension point="org.eclipse.emf.edit.childCreationExtenders">
      <extender
            uri="ca.gc.asc_csa.eclipse.emf.ecore.invocator"
            class="org.eclipse.symphony.core.recordings.provider.EMFEcoreInvocatorRecordingsItemProviderAdapterFactory$EMFEcoreInvocatorChildCreationExtender"/>
   </extension>

</plugin>
