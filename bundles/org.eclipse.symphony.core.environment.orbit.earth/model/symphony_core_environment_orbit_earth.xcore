	@GenModel(notify="true", property="Editable",  propertyCategory="TIME_PERIOD", csa_unit = "s")
	double timeInterval = "1.0"
	
	/*
	 * The OrbitModel to use to predict the corridor.
	 */	
	@GenModel(propertyCategory="ORBIT_MODEL")
