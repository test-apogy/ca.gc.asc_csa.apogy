/*
 * Ephemeris types defined for Two-Lines Elements
 */
enum EphemerisType
{
	DEFAULT as "DEFAULT" = 0,
	SGP as "SGP" = 1,
	SGP4 as "SGP4" = 2,
	SDP4 as "SDP4" = 3,
	SGP8 as "SGP8" = 4,
	SDP8 as "SDP8" = 5
}



