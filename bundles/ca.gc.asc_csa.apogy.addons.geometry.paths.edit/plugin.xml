<plugin>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="ca.gc.asc_csa.apogy.addons.geometry.paths"
            class="ca.gc.asc_csa.apogy.addons.geometry.paths.provider.ApogyAddonsGeometryPathsItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>

   <extension point="org.eclipse.emf.edit.childCreationExtenders">
      <!-- @generated apogy_addons_geometry_paths -->
      <extender
            uri="ca.gc.asc_csa.apogy.common.topology"
            class="ca.gc.asc_csa.apogy.addons.geometry.paths.provider.ApogyAddonsGeometryPathsItemProviderAdapterFactory$ApogyCommonTopologyChildCreationExtender"/>
      <extender
            uri="ca.gc.asc_csa.apogy.common.geometry.data"
            class="ca.gc.asc_csa.apogy.addons.geometry.paths.provider.ApogyAddonsGeometryPathsItemProviderAdapterFactory$ApogyCommonGeometryDataChildCreationExtender"/>
   </extension>

</plugin>
