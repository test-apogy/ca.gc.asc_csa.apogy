            uri="org.eclipse.symphony.common.topology.addons,dynamics"
            class="org.eclipse.symphony.common.topology.addons.dynamics.provider.TopologyDynamicsItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>

   <extension point="org.eclipse.emf.edit.childCreationExtenders">
      <extender
            uri="org.eclipse.symphony.common.topology.addons.dynamics"
            class="org.eclipse.symphony.common.topology.addons.dynamics.provider.TopologyDynamicsItemProviderAdapterFactory$TopologyChildCreationExtender"/>
