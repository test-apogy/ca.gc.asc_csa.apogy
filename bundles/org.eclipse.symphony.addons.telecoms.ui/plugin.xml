            uri="org.eclipse.symphony.addons.telecoms.ui"
            class="org.eclipse.symphony.addons.telecoms.ui.SymphonyTelecomToolsUIPackage"
            genModel="model/symphony_addons_telecoms_ui.xcore"/>
   </extension>
   <extension
         point="org.eclipse.symphony.common.topology.ui.presentation">
      <Class
            Class="org.eclipse.symphony.addons.telecoms.ui.adapters.AbstractAntennaRadiationPatternAdapter">
      </Class>
   </extension>

</plugin>
