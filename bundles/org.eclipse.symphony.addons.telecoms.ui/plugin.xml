            category="org.eclipse.symphony.addons.ui.category"
            class="org.eclipse.symphony.addons.telecoms.ui.views.TelecomStatusMonitorToolView"
            id="org.eclipse.symphony.addons.telecoms.ui.views.TelecomStatusMonitorToolView"
