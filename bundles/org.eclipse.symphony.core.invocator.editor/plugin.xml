<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.eclipse.symphony.core.invocator.presentation.EMFEcoreInvocatorModelWizardID"
            name="%_UI_EMFEcoreInvocatorModelWizard_label"
            class="org.eclipse.symphony.core.invocator.presentation.EMFEcoreInvocatorModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/EMFEcoreInvocatorModelFile.gif">
         <description>%_UI_EMFEcoreInvocatorModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="org.eclipse.symphony.core.invocator.presentation.EMFEcoreInvocatorEditorID"
            name="%_UI_EMFEcoreInvocatorEditor_label"
            icon="platform:/plugin/org.eclipse.symphony.core.ui/icons/symphony_logo_16x16.gif"
            extensions="ei, ss, sym"
            class="org.eclipse.symphony.core.invocator.presentation.CustomEMFEcoreInvocatorEditor"
            contributorClass="org.eclipse.symphony.core.invocator.presentation.EMFEcoreInvocatorActionBarContributor">
      </editor>
   </extension>

</plugin>
