<plugin>

  <extension point="org.eclipse.emf.ecore.generated_package">
    <package 
       uri = "http:///ca/gc/space/mrt/geometry/data3d.ecore" 
       class = "ca.gc.space.mrt.geometry.data3d.Data3dPackage"
       genModel = "model/data3d.genmodel" /> 
  </extension>
  <extension
        point="org.eclipse.symphony.common.converters">
     <new_element
           Class="ca.gc.space.mrt.geometry.data3d.converters.TopoIFileToCartesianTriangularMeshConverter">
     </new_element>
     <new_element
           Class="ca.gc.space.mrt.geometry.data3d.converters.TopoFileToCartesianTriangularMeshConverter">
     </new_element>
     <new_element
           Class="ca.gc.space.mrt.geometry.data3d.converters.URLTRIToCartesianTriangularMeshConverter">
     </new_element>
     <new_element
           Class="ca.gc.space.mrt.geometry.data3d.converters.URLTopoToCartesianTriangularMeshConverter">
     </new_element>
  </extension>

</plugin>
