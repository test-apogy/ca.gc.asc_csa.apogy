<plugin>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="http:///ca/gc/space/mrt/pathplanners/pathplanners.ecore"
            class="org.eclipse.symphony.addons.mobility.pathplanners.provider.PathplannersItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>

   <extension point="org.eclipse.emf.edit.childCreationExtenders">
      <!-- @generated symphony_addons_mobility_pathplanners -->
      <extender
            uri="org.eclipse.symphony.common.topology"
            class="org.eclipse.symphony.addons.mobility.pathplanners.provider.PathplannersItemProviderAdapterFactory$TopologyChildCreationExtender"/>
      <extender
            uri="org.eclipse.symphony.addons.geometry.paths"
            class="org.eclipse.symphony.addons.mobility.pathplanners.provider.PathplannersItemProviderAdapterFactory$PathsChildCreationExtender"/>
   </extension>

</plugin>
