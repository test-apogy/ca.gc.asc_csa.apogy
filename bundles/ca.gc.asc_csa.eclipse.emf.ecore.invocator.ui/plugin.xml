            class="ca.gc.asc_csa.eclipse.emf.ecore.invocator.ui.views.InstanceView"
            icon="platform:/plugin/ca.gc.asc_csa.eclipse.emf.ecore.invocator.edit/icons/full/obj16/Instance.gif"
            id="ca.gc.asc_csa.eclipse.emf.ecore.invocator.ui.views.InstanceView"
