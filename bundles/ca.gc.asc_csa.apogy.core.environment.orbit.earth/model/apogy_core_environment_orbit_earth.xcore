{
	/*
	 * The first line of the TLE in text form.
	 */
	@GenModel(property="Editable", propertyCategory="TLE_RAW_DATA", notify="true")
	String[1] firstLine
	
	/*
	 * The second line of the TLE in text form.
	 */
	@GenModel(property="Editable", propertyCategory="TLE_RAW_DATA", notify="true")
	String[1] secondLine
}

