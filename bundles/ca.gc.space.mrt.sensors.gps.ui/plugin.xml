<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="ca.gc.space.mrt.sensors.gps.ui.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="RCP Perspective"
            class="ca.gc.space.mrt.sensors.gps.ui.Perspective"
            id="ca.gc.space.mrt.sensors.gps.ui.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="ca.gc.space.mrt.sensors.gps.ui.gpsCategory"
            class="ca.gc.space.mrt.sensors.gps.ui.views.GPSView"
            id="ca.gc.space.mrt.sensors.gps.ui.views.GPSView"
            name="GPS View"
            restorable="true">
      </view>
      <category
            id="ca.gc.space.mrt.sensors.gps.ui.gpsCategory"
            name="GPS">
      </category>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="ca.gc.space.mrt.sensors.gps.ui.application"
            name="GPS">
         <property
               name="appName"
               value="GPS">
         </property>
      </product>
   </extension>

</plugin>
