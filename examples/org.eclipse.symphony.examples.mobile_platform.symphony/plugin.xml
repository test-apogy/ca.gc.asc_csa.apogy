<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated symphony_examples_mobile_platform -->
      <package
            uri="org.eclipse.symphony.examples.mobile_platform.symphony"
            class="org.eclipse.symphony.examples.mobile_platform.symphony.SymphonyExampleMobilePlatformPackage"
            genModel="model/symphony_examples_mobile_platform.xcore"/>
   </extension>
   <extension
         point="org.eclipse.symphony.core.invocator.TypeContributor">
      <Type
            URI="model/MobilePlatformSymphonySystem.ss">
      </Type>
   </extension>
   <extension
         point="org.eclipse.symphony.core.invocator.SessionProvider">
      <SessionProvider
            Description="Session showing the use of the Mobile Platform example system."
            Name="Mobile Platform Example"
            URI="sessions/MobilePlatformExampleSession.sym">
      </SessionProvider>
   </extension>

</plugin>
