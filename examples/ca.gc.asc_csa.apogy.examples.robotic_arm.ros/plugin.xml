            uri="ca.gc.asc_csa.apogy.examples.robotic_arm.ros"
            class="ca.gc.asc_csa.apogy.examples.robotic_arm.ros.ApogyExamplesRoboticArmROSPackage"
            genModel="model/apogy_examples_robotic_arm_ros.xcore"/>
