<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated symphony_examples_rover -->
      <package
            uri="org.eclipse.symphony.examples.rover.symphony"
            class="org.eclipse.symphony.examples.rover.symphony.Symphony__ExamplesRoverSymphonyPackage"
            genModel="model/symphony_examples_rover_symphony.xcore"/>
   </extension>
   <extension
         point="org.eclipse.symphony.core.invocator.typeContributor">
      <Type
            URI="model/RoverSymphonySystem.ss">
      </Type>
   </extension>
   <extension
         point="org.eclipse.symphony.core.invocator.sessionProvider">
      <SessionProvider
            Description="Session showing the use of the Rover example system."
            Name="Rover Example"
            URI="sessions/RoverExampleSession.sym">
      </SessionProvider>
   </extension>

</plugin>
