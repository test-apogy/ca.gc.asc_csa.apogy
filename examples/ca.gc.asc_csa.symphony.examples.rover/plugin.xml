            uri="ca.gc.asc_csa.symphony.examples.rover"
            class="ca.gc.asc_csa.symphony.examples.rover.SymphonyExampleRoverPackage"
            genModel="model/symphony_examples_rover.xcore"/>
   </extension>
   <extension
         point="ca.gc.asc_csa.eclipse.emf.ecore.invocator.TypeContributor">
      <Type
            URI="model/RoverSymphonySystem.ss">
      </Type>
   </extension>
   <extension
         point="ca.gc.asc_csa.eclipse.emf.ecore.invocator.SessionProvider">
      <SessionProvider
            Description="Session showing the use of the Rover example system."
            Name="Rover Example"
            URI="sessions/RoverExampleSession.sym">
      </SessionProvider>
   </extension>

</plugin>
