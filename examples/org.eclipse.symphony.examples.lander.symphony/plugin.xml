<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated symphony_examples_lander -->
      <package
            uri="org.eclipse.symphony.examples.lander.symphony"
            class="org.eclipse.symphony.examples.lander.symphony.SymphonyExampleLanderPackage"
            genModel="model/symphony_examples_lander.xcore"/>
   </extension>
   <extension
         point="org.eclipse.symphony.core.invocator.TypeContributor">
      <Type
            URI="model/LanderSymphonySystem.ss">
      </Type>
   </extension>
   <extension
         point="org.eclipse.symphony.core.invocator.SessionProvider">
      <SessionProvider
            Description="Session showing the use of the Lander example system."
            Name="Lander Example"
            URI="sessions/LanderExampleSession.sym">
      </SessionProvider>
   </extension>

</plugin>
