            uri="http:///ca/gc/space/topology.ecore"
            class="ca.gc.asc_csa.eclipse.emf.ecore.examples.rover.provider.EMFEcoreExampleRoverItemProviderAdapterFactory$TopologyChildCreationExtender"/>
