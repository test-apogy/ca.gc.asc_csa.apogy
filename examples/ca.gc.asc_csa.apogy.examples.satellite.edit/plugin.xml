<plugin>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <!-- @generated symphony_examples_satellite -->
      <factory
            uri="org.eclipse.symphony.examples.satellite"
            class="org.eclipse.symphony.examples.satellite.provider.Symphony__ExamplesSatelliteItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>

   <extension point="org.eclipse.emf.edit.childCreationExtenders">
      <!-- @generated symphony_examples_satellite -->
      <extender
            uri="org.eclipse.symphony.core.environment.orbit.earth"
            class="org.eclipse.symphony.examples.satellite.provider.Symphony__ExamplesSatelliteItemProviderAdapterFactory$Symphony__CoreEnvironmentOrbitEarthChildCreationExtender"/>
      <extender
            uri="org.eclipse.symphony.core.environment"
            class="org.eclipse.symphony.examples.satellite.provider.Symphony__ExamplesSatelliteItemProviderAdapterFactory$Symphony__CoreEnvironmentChildCreationExtender"/>
      <extender
            uri="org.eclipse.symphony.common.images"
            class="org.eclipse.symphony.examples.satellite.provider.Symphony__ExamplesSatelliteItemProviderAdapterFactory$Symphony__CommonImagesChildCreationExtender"/>
      <extender
            uri="org.eclipse.symphony.common.topology"
            class="org.eclipse.symphony.examples.satellite.provider.Symphony__ExamplesSatelliteItemProviderAdapterFactory$Symphony__CommonTopologyChildCreationExtender"/>
   </extension>

</plugin>
