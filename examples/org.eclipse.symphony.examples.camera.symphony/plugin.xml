<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated symphony_examples_camera -->
      <package
            uri="org.eclipse.symphony.examples.camera.symphony"
            class="org.eclipse.symphony.examples.camera.symphony.SymphonyExampleCameraPackage"
            genModel="model/symphony_examples_camera_symphony.xcore"/>
   </extension>
   <extension
         point="ca.gc.asc_csa.eclipse.emf.ecore.invocator.TypeContributor">
      <Type
            URI="model/CameraSymphonySystem.ss">
      </Type>
   </extension>
   
   <extension
         point="ca.gc.asc_csa.eclipse.emf.ecore.invocator.TypeContributor">
      <Type
            URI="model/PTUCameraSymphonySystem.ss">
      </Type>
   </extension>
   <extension
         point="ca.gc.asc_csa.eclipse.emf.ecore.invocator.SessionProvider">
      <SessionProvider
            Description="Session showing the use of the Camera example system."
            Name="Camera Example"
            URI="sessions/CameraExampleSession.sym">
      </SessionProvider>
   </extension>
   <extension
         point="ca.gc.asc_csa.eclipse.emf.ecore.invocator.SessionProvider">
      <SessionProvider
            Description="Session showing the use of the PTU Camera example system."
            Name="PTU Camera Example"
            URI="sessions/PTUCameraExampleSession.sym">
      </SessionProvider>
   </extension>
   

</plugin>
